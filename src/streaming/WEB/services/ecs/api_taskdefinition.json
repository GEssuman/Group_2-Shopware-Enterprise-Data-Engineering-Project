{
  "family": "weblogs-api",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::985539772768:role/lab6-ecs-task-execution-role",
  "taskRoleArn": "arn:aws:iam::985539772768:role/ecsTaskExecutionRole-weblogs",
  "containerDefinitions": [
    {
      "name": "api-container",
      "image": "985539772768.dkr.ecr.us-east-1.amazonaws.com/weblogs/api-gtp:api",
      "essential": true,
      "environment": [
        {
          "name": "API_URL",
          "value": "http://4.328.189.26:8000/api/-traffic/"
        },
        {
          "name": "KINESIS_STREAM_NAME",
          "value": "weblogs_stream"
        },
        {
          "name": "POLL_INTERVAL",
          "value": "6"
        },
        {
          "name": "REGION",
          "value": "us-east-1"
        },
          {
          "name": "FAILED_RECORDS_SNS_ARN",
          "value": "arn:aws:sns:us-east-1:985539772768:validation_fail_weblogs:3a6b908f-0dad-4c85-96ac-3f5e03f00a80"
        },
          {
          "name": "ERROR_S3_BUCKET",
          "value": "weblogs-bucket-gtp"
        }
      ],

      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-region": "us-east-1",
          "awslogs-group": "/ecs/api-task",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
